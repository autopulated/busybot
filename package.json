{
  "name": "busybot",
  "version": "1.0.3",
  "description": "Impose non-parallelisable proof of work to slow down pesky bots.",
  "type": "module",
  "license": "Apache-2.0",
  "files": [
    "index.mjs"
  ],
  "directories": {
    "test": "test"
  },
  "keywords": [
    "pow",
    "proof-of-work",
    "kctf",
    "non-parallel",
    "rate-limit",
    "ratelimit",
    "modular exponentiation",
    "modular square root"
  ],
  "module": "index.mjs",
  "main": "index.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/autopulated/busybot.git"
  },
  "scripts": {
    "test": "tap run",
