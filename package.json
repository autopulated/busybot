    "allow-incomplete-coverage": true,
    "coverage-report": [
      "text",
      "lcovonly"
    ],
    "files": [
      "test"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "eslint": "^9.34.0",
    "tap": "^21.1.0"
  },
  "bugs": {
